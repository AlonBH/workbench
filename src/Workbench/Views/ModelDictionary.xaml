<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Workbench.ViewModels"
                    xmlns:controls="clr-namespace:Workbench.Controls">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ConstraintDictionary.xaml"/>
        <ResourceDictionary Source="DomainDictionary.xaml"/>
        <ResourceDictionary Source="VariableDictionary.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    <DataTemplate DataType="{x:Type viewModels:ModelViewModel}">
        <controls:GraphicDisplayControl />
    </DataTemplate>

</ResourceDictionary>
    